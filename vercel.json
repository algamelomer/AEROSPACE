{
    "version": 2,
    "rewrites": [
        {
            "source": "/api/(.*)",
            "destination": "/api/index.ts"
        },
        {
            "source": "/(.*)",
            "destination": "/dist/public/$1",
            "check": true
        },
        {
            "source": "/(.*)",
            "destination": "/dist/public/index.html"
        }
    ]
}